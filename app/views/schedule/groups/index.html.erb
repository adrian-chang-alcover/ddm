<h1>Listing schedule_groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Capacity</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_groups.each do |schedule_group| %>
      <tr>
        <td><%= schedule_group.name %></td>
        <td><%= schedule_group.capacity %></td>
        <td><%= schedule_group.year_id %></td>
        <td><%= link_to 'Show', schedule_group %></td>
        <td><%= link_to 'Edit', edit_schedule_group_path(schedule_group) %></td>
        <td><%= link_to 'Destroy', schedule_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Group', new_schedule_group_path %>
