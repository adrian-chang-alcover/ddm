<h1>Listing schedule_p1s</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Docent week</th>
      <th>Turn type</th>
      <th>Name</th>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_p1s.each do |schedule_p1| %>
      <tr>
        <td><%= schedule_p1.subject_id %></td>
        <td><%= schedule_p1.docent_week_id %></td>
        <td><%= schedule_p1.turn_type_id %></td>
        <td><%= schedule_p1.name %></td>
        <td><%= schedule_p1.number %></td>
        <td><%= link_to 'Show', schedule_p1 %></td>
        <td><%= link_to 'Edit', edit_schedule_p1_path(schedule_p1) %></td>
        <td><%= link_to 'Destroy', schedule_p1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New P1', new_schedule_p1_path %>
