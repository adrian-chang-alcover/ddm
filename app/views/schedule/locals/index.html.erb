<h1>Listing schedule_locals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Capacity</th>
      <th>Faculty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_locals.each do |schedule_local| %>
      <tr>
        <td><%= schedule_local.name %></td>
        <td><%= schedule_local.capacity %></td>
        <td><%= schedule_local.faculty_id %></td>
        <td><%= link_to 'Show', schedule_local %></td>
        <td><%= link_to 'Edit', edit_schedule_local_path(schedule_local) %></td>
        <td><%= link_to 'Destroy', schedule_local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Local', new_schedule_local_path %>
