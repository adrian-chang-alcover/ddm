<h1>Listing schedule_docent_days</h1>

<table>
  <thead>
    <tr>
      <th>Day</th>
      <th>Docent week</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_docent_days.each do |schedule_docent_day| %>
      <tr>
        <td><%= schedule_docent_day.day %></td>
        <td><%= schedule_docent_day.docent_week_id %></td>
        <td><%= link_to 'Show', schedule_docent_day %></td>
        <td><%= link_to 'Edit', edit_schedule_docent_day_path(schedule_docent_day) %></td>
        <td><%= link_to 'Destroy', schedule_docent_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Docent day', new_schedule_docent_day_path %>
